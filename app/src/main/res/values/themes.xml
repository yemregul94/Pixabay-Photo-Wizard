<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.TSoft_Pixabay" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/theme_primary</item>
        <item name="colorPrimaryVariant">@color/theme_primary_variant</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/theme_secondary</item>
        <item name="colorSecondaryVariant">@color/theme_secondary</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="colorOnError">@color/red</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/theme_primary</item>
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
    </style>
</resources>